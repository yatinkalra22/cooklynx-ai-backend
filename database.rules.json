{
  "rules": {
    "images": {
      ".indexOn": ["userId"]
    },
    "analysis": {
      ".indexOn": ["userId"]
    },
    "videos": {
      ".indexOn": ["userId"]
    },
    "videoAnalysis": {
      ".indexOn": ["userId"]
    },
    "videoHashes": {
      "$userId": {
        ".indexOn": [".value"]
      }
    },
    "urlExtractions": {
      ".indexOn": ["userId"]
    },
    "urlRecipes": {
      ".indexOn": ["userId"]
    },
    "sharedUrlRecipes": {
      ".read": "auth != null",
      ".indexOn": ["normalizedUrl"]
    }
  }
}
